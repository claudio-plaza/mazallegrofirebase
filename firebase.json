{
  "emulators": {
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8081
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true,
      "port": 4000
    }
  },
  "functions": {
    "source": "functions",
    "runtime": "nodejs20"
  },
  "firestore": {
    "rules": "firestore.rules",
    "database": "(default)"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "site": "clubzenith-efa8b",
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/node_modules/**",
      ".git/**",
      ".firebase/**",
      ".next/**",
      "**/package-lock.json",
      "**/package.json",
      "**/tsconfig.json",
      "**/postcss.config.mjs",
      "**/tailwind.config.ts",
      "**/next.config.mjs",
      "**/next.config.ts",
      "**/components.json",
      "**/apphosting.yaml",
      "**/README.md",
      "**/docs/**",
      "**/src/ai/**",
      "**/workspace/**"
    ]
